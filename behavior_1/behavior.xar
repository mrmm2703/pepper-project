import urllib, json
class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.tts = ALProxy("ALTextToSpeech")
        self.url = "https://api.tfl.gov.uk/Line/Mode/tube/Status?detail=false&app_id=5ebbac01&app_key=4449890e71d65f6f084ef78529cd9511"

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        #self.onStopped() #activate the output of the box
        try:
            response = urllib.urlopen(self.url)
            data = json.loads(response.read())

            # Initiate a list of statuses
            listOfStatus = []

            # Loop through every service
            for line in data:
                listOfStatus.append(line['lineStatuses'][0]['statusSeverityDescription'])

            isOther = False
            # Check if there are any not good services
            for x in listOfStatus:
                if x != "Good Service":
                    isOther = True

            # If there are other non good services
            if isOther:
                self.tts.say("Here are the following disruptions")
                for line in data:
                    if line['lineStatus'][0]['statusSeverityDescription'] != "Good Service":
                        self.tts.say(line['name'] + ": " + line['lineStatuses'][0]['statusSeverityDescription'])
            # Otherwise there are no disruptions
            else:
                self.tts.say("There are no disruptions on the tube.")
        except:
            self.tts.say("Could not get data from TFL")

